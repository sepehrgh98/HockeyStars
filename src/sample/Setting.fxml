<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@Setting.css" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.settingConroller">
    <children>
        <Slider fx:id="volumeslider" layoutX="73.0" layoutY="325.0" prefHeight="14.0" prefWidth="483.0" />
        <RadioButton fx:id="rb1" layoutX="368.0" layoutY="42.0" mnemonicParsing="false" text="music 1" textFill="#0010ea">
            <toggleGroup>
                <ToggleGroup fx:id="mygroup" />
            </toggleGroup>
            <font>
                <Font size="25.0" />
            </font>
        </RadioButton>
        <RadioButton fx:id="rb2" layoutX="368.0" layoutY="122.0" mnemonicParsing="false" text="music 2" textFill="#3900e2" toggleGroup="$mygroup">
            <font>
                <Font size="25.0" />
            </font></RadioButton>
        <Button fx:id="Playbtn" layoutX="115.0" layoutY="23.0" mnemonicParsing="false" prefHeight="76.0" prefWidth="82.0" />
        <Button fx:id="Stopbtn" layoutX="115.0" layoutY="129.0" mnemonicParsing="false" prefHeight="76.0" prefWidth="82.0" />
        <Label layoutX="14.0" layoutY="318.0" text="Sound" textFill="#1b09e5">
            <font>
                <Font size="19.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
